{
  "plugins": [
		"stylelint-prettier",
    "stylelint-order",
    "stylelint-config-rational-order/plugin"
  ],
  "rules": {
		"prettier/prettier": true,
    "order/properties-order": [],
    "plugin/rational-order": [true, {
      "border-in-box-model": false,
      "empty-line-between-groups": true,
    }]
  }
}