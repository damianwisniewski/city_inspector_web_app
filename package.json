{
	"name": "city_inspector_web_api",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"lint": "eslint src/**/*.js src/**/*.jsx scripts/**/*.js",
		"test:unit": "react-scripts test --testMatch '<rootDir>/src/**/?(*.)unit.test.{js,jsx}'",
		"test:unit:watch": "npm run test:unit -- --watch",
		"test:e2e": "wdio wdio.conf.js",
		"release": "git push heroku master"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie < 11",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run test"
		}
	},
	"dependencies": {
		"leaflet": "^1.5.1",
		"leaflet.markercluster": "^1.4.1",
		"node-sass": "^4.12.0",
		"prop-types": "^15.7.2",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-leaflet": "^2.4.0",
		"react-leaflet-markercluster": "^2.0.0-rc3",
		"react-redux": "^6.0.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "2.1.8",
		"redux": "^4.0.4",
		"redux-saga": "^1.1.1",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.3",
		"@types/jest": "^24.0.18",
		"@wdio/cli": "^5.14.3",
		"@wdio/local-runner": "^5.14.3",
		"@wdio/mocha-framework": "^5.14.3",
		"@wdio/selenium-standalone-service": "^5.13.2",
		"@wdio/spec-reporter": "^5.13.2",
		"@wdio/sync": "^5.14.3",
		"chai": "^4.2.0",
		"chalk": "^2.4.2",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.14.0",
		"enzyme-to-json": "^3.4.2",
		"eslint-config-airbnb": "^17.1.1",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-chai-friendly": "^0.4.1",
		"eslint-plugin-compat": "^3.3.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-webdriverio": "^1.0.1",
		"faker": "^4.1.0",
		"husky": "^1.3.1",
		"json-server": "^0.14.2",
		"prettier": "1.16.4",
		"react-test-renderer": "^16.10.2",
		"redux-devtools-extension": "^2.13.8",
		"redux-mock-store": "^1.5.3",
		"redux-saga-test": "^1.0.1",
		"stylelint": "^11.1.1",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-order": "^3.1.1",
		"webdriverio": "^5.14.3"
	}
}
