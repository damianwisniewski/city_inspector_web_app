{
	"name": "city_inspector_web_api",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"browserslist": "echo 'Supported browsers:\n' && browserslist",
		"lint": "eslint src/**/*.js src/**/*.jsx scripts/**/*.js",
		"lint:fix": "npm run lint -- --fix",
		"stylelint": "stylelint src/**/*.scss",
		"stylelint:fix": "npm run stylelint -- --fix",
		"test:unit": "react-scripts test --testPathPattern='src/.*/__tests__/.*.unit.test.(jsx|js)' --watchAll=false",
		"test:unit:watch": "npm run test:unit -- --watchAll=true",
		"test:e2e": "wdio wdio.conf.js",
		"release": "git push heroku master"
	},
	"browserslist": [
		">1%",
		"not dead",
		"not ie < 11",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run stylelint && npm run test:unit"
		}
	},
	"dependencies": {
		"core-js": "^3.3.6",
		"leaflet": "^1.5.1",
		"leaflet.markercluster": "^1.4.1",
		"node-sass": "^4.13.0",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-leaflet": "^2.5.0",
		"react-leaflet-markercluster": "^2.0.0-rc3",
		"react-redux": "^6.0.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0",
		"redux": "^4.0.4",
		"redux-saga": "^1.1.1",
		"stylelint-config-standard": "^19.0.0",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@types/enzyme": "^3.10.3",
		"@types/jest": "^24.0.21",
		"@wdio/cli": "^5.15.6",
		"@wdio/local-runner": "^5.15.6",
		"@wdio/mocha-framework": "^5.15.1",
		"@wdio/selenium-standalone-service": "^5.15.0",
		"@wdio/spec-reporter": "^5.15.2",
		"@wdio/sync": "^5.15.7",
		"chai": "^4.2.0",
		"chalk": "^2.4.2",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"enzyme-to-json": "^3.4.3",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-chai-friendly": "^0.4.1",
		"eslint-plugin-compat": "3.3.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"eslint-plugin-webdriverio": "^1.0.1",
		"faker": "^4.1.0",
		"husky": "^1.3.1",
		"json-server": "^0.14.2",
		"prettier": "1.16.4",
		"react-test-renderer": "^16.11.0",
		"redux-devtools-extension": "^2.13.8",
		"redux-mock-store": "^1.5.3",
		"redux-saga-test": "^1.0.1",
		"source-map-explorer": "^2.1.0",
		"stylelint": "^11.1.1",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-order": "^3.1.1",
		"stylelint-prettier": "^1.1.1",
		"webdriverio": "^5.15.6"
	}
}