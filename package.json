{
	"name": "city_inspector_web_api",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"mock-api": "json-server --watch mockServerRestApi/index.js --port 4000",
		"lint": "eslint src/**/*.js src/**/*.jsx scripts/**/*.js",
		"test": "react-scripts test --testMatch '<rootDir>/src/**/?(*.)unit.test.{js,jsx}'",
		"test:e2e": "wdio wdio.conf.js",
		"release": "git push heroku master"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie < 11",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run test"
		}
	},
	"dependencies": {
		"eslint-plugin-webdriverio": "^1.0.1",
		"node-sass": "^4.11.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"react-redux": "^6.0.1",
		"react-router-dom": "^5.0.0",
		"react-scripts": "2.1.8",
		"redux": "^4.0.1",
		"redux-saga": "^1.0.2",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@types/enzyme": "^3.9.1",
		"@types/jest": "^24.0.11",
		"@wdio/cli": "^5.7.3",
		"@wdio/local-runner": "^5.7.3",
		"@wdio/mocha-framework": "^5.7.3",
		"@wdio/selenium-standalone-service": "^5.7.2",
		"@wdio/spec-reporter": "^5.7.2",
		"@wdio/sync": "^5.7.3",
		"chai": "^4.2.0",
		"chalk": "^2.4.2",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.11.2",
		"enzyme-to-json": "^3.3.5",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-chai-friendly": "^0.4.1",
		"eslint-plugin-compat": "^3.1.0",
		"eslint-plugin-import": "^2.16.0",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"faker": "^4.1.0",
		"husky": "^1.3.1",
		"json-server": "^0.14.2",
		"prettier": "1.16.4",
		"react-test-renderer": "^16.8.6",
		"redux-devtools-extension": "^2.13.8",
		"redux-mock-store": "^1.5.3",
		"redux-saga-test": "^1.0.1",
		"webdriverio": "^5.7.3"
	}
}
